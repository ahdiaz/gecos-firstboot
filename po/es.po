#: ../firstboot/pages/linkToServer/ServerConf.py:289
msgid "LDAP setup error"
msgstr "Error en la configuración del LDAP"

#: ../firstboot/pages/linkToServer/ServerConf.py:173
#: ../firstboot/pages/linkToServer/ServerConf.py:342
msgid "The Chef configuration script couldn't be found"
msgstr "No se pudo encontrar el script de configuración de Chef"

#: ../firstboot/pages/linkToServer/ServerConf.py:188
#: ../firstboot/pages/linkToServer/ServerConf.py:352
msgid "Chef setup error"
msgstr "Error en la configuración de Chef"

#: ../firstboot/pages/linkToServer/ServerConf.py:212
msgid "Workstation has been unlinked from LDAP."
msgstr "Este puesto de trabajo se ha desvinculado del LDAP."

#: ../firstboot/pages/linkToServer/ServerConf.py:222
msgid "The LDAP has been configured successfully."
msgstr "El LDAP se ha configurado correctamente."

#: ../firstboot/pages/linkToServer/ServerConf.py:232
msgid "Workstation has been unlinked from Chef."
msgstr "Este puesto de trabajo se ha desvinculado de Chef."

#: ../firstboot/pages/linkToServer/ServerConf.py:242
msgid "The Chef client has been configured successfully."
msgstr "El cliente de Chef se ha configurado correctamente."

#: ../firstboot/pages/linkToServer/ServerConf.py:264
msgid "The LDAP URL cannot be empty."
msgstr "La URL del LDAP no puede estar vacía."

#: ../firstboot/pages/linkToServer/ServerConf.py:267
msgid "The LDAP BaseDN cannot be empty."
msgstr "El BaseDN del LDAP no puede estar vacío."

#: ../firstboot/pages/linkToServer/ServerConf.py:270
msgid "The LDAP BindDN cannot be empty."
msgstr "El BindDN del LDAP no puede estar vacío."

#: ../firstboot/pages/linkToServer/ServerConf.py:312
msgid "An error has ocurred unlinking from LDAP"
msgstr "Ha ocurrido un error desvinculándose del LDAP"

#: ../firstboot/pages/linkToServer/ServerConf.py:327
msgid "The Chef URL cannot be empty."
msgstr "LA URL de Chef no puede quedar vacía."

#: ../firstboot/pages/linkToServer/ServerConf.py:330
msgid "The Chef certificate URL cannot be empty."
msgstr "La URL del certificado de Chef no puede quedar vacía."

#: ../firstboot/pages/linkToServer/ServerConf.py:333
msgid "The Chef host name cannot be empty."
msgstr "El nombre de host para Chef no puede quedar vacío."

#: ../firstboot/pages/linkToServer/ServerConf.py:375
msgid "An error has ocurred unlinking from Chef"
msgstr "Ha ocurrido un error desvinculándose de Chef"

#: ../firstboot/pages/linkToServer/LinkToServerConfEditorPage.py:34
